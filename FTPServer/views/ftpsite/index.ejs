          <th>Genre</th>
          <th>Movie Name</th>
          <th>Rating</th>
          <th>Publish Year</th>
          <th>Action</th>

        </tr>

        <% for(var i=0; i< actionMovies.length; i++ ){ %>
          
        <tr id="rows">
          <td><%= actionMovies[i].genre %></td>
          <td><%= actionMovies[i].name %></td>
          <td><%= actionMovies[i].rating %></td>
          <td><%= actionMovies[i].publishYear %></td>
          <td>
          <a href="/ftpsite/movieDownload/<%=actionMovies[i].id%>"class="w3-bar-item w3-button tablink" >Download</a>
          </td>
        </tr>
        <% } %>

       </table>
